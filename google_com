                    var optionHash = Options._currentHash; // TODO shouldn't be accessing _ property

                    var url = this.href;
                    if (filterHash) {
                        url += "?filter=" + encodeURIComponent(filterHash);
                    }

                    if (optionHash) {
                        url += (filterHash ? "&" : "?");
                        url += "options=" + encodeURIComponent(optionHash);
                    }

                    window.location.href = url;
                    e.preventDefault();
                    return false;
                })
            });
        </script>
                <script>
            document.addEventListener("DOMContentLoaded", function(){
                Highlights.init("/ajax/data/highlights2.php");
            });
        </script>
                    </div>
        </div>

                    
        </div>

