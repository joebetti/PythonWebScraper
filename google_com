                // TODO we can probably write this more generalized - use Filters and Options events,
                // make it more generalized. Right now this is a "hotfix"
                document.getElementById("rss-link").addEventListener("click", function(e) {
                    var filterHash = Filters.getHash();
                    var optionHash = Options._currentHash; // TODO shouldn't be accessing _ property

                    var url = this.href;
                    if (filterHash) {
                        url += "?filter=" + encodeURIComponent(filterHash);
                    }

                    if (optionHash) {
                        url += (filterHash ? "&" : "?");
                        url += "options=" + encodeURIComponent(optionHash);
                    }

                    window.location.href = url;
                    e.preventDefault();
                    return false;
                })
            });
